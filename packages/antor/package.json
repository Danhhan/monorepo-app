    "babel-plugin-formatjs": "^10.2.6",
    "babel-plugin-react-intl": "^8.2.25",
    "postcss": "^7.0.35",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.1.0",
    "ts-loader": "^8.1.0"
  }
